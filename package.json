{
  "name": "@crystal-ball/prettier-base",
  "version": "1.0.0",
  "description": "Fellowship shared Prettier configs",
  "main": "index.js",
  "keywords": [
    "Prettier",
    "Crystal Ball"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "scripts": {},
  "dependencies": {
    "prettier": "~1.17.0"
  },
  "devDependencies": {
    "commitizen": "3.1.1",
    "cz-adapter-eslint": "0.2.0",
    "husky": "2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  }
}
