{
  "name": "@crystal-ball/prettier-base",
  "version": "1.0.0",
  "description": "Fellowship shared Prettier configs",
  "main": "index.js",
  "keywords": [
    "Prettier",
    "Crystal Ball"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/prettier-base.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "scripts": {
    "test": "echo \"No tests for package\""
  },
  "dependencies": {
    "prettier": "1.17.0"
  },
  "devDependencies": {
    "@crystal-ball/commit-semantics": "1.0.0",
    "husky": "2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-adapter"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "prettier": "./index.js",
  "release": {
    "extends": [
      "@crystal-ball/semantic-release-base"
    ]
  }
}
